        "type": "object",
        "properties": {
          "id": {
            "type": "number",
            "example": 1
          },
          "length": {
            "type": "number",
            "example": 60
          },
          "slug": {
            "type": "string",
            "example": "cooking-class"
          },
          "title": {
            "type": "string",
            "example": "Learn the secrets of masterchief!"
          },
          "description": {
            "type": "string",
            "nullable": true,
            "example": "Discover the culinary wonders of the Argentina by making the best flan ever!"
          },
          "locations": {
            "nullable": true,
            "allOf": [
              {
                "$ref": "#/components/schemas/EventTypeLocation"
              }
            ]
          }
        },
        "required": [
          "id",
          "length",
          "slug",
          "title",
          "description",
          "locations"
        ]
      },
