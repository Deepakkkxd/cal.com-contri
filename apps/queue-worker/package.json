{
  "name": "@calcom/queue-worker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc --build",
    "build:watch": "tsc --build --watch",
    "start": "tsx src/worker.ts",
    "dev": "nodemon src/worker.ts",
    "test": "mocha --exit --require tsx --require source-map-support/register tests/*.test.ts"
  },
  "nodemonConfig": {
    "execMap": {
      "ts": "tsx"
    },
    "ext": "ts",
    "watch": [
      "src"
    ]
  },
  "dependencies": {
    "@calcom/emails": "*",
    "@temporalio/activity": "^1.8.6",
    "@temporalio/client": "^1.8.6",
    "@temporalio/worker": "^1.8.6",
    "@temporalio/workflow": "^1.8.6",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@calcom/tsconfig": "*",
    "@calcom/types": "*",
    "@temporalio/testing": "^1.8.6",
    "@tsconfig/node20": "20.1.2",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.2",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.2",
    "tsx": "^4.6.2",
    "typescript": "^5.3.2"
  }
}
